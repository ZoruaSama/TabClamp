{
  "manifest_version": 2,

  "name": "标签夹 Tab Clamp",
  "description": "帮助您一键打开多个标签，高效学习工作",
  "version": "2.0.0",
  "icons":{
       "128":"icon.png","16":"icon.png"
   },
  "permissions": [
    "tabs",	
    "actionTab",
    "http://*/*",
    "https://*/*"
  ],
   "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "update_url": "http://upext.chrome.360.cn/intf.php?method=ExtUpdate.query"
}
